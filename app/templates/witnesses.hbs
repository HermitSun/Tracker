<h1>Witnesses</h1>
<div class="row">
    {{#each model as |witness|}}
        <div class="col-xs-12 col-sm-6 col-md-4">
            <div class="well">
                <div class="caption">
                    <h3>{{witness.fullName}}</h3>
                    <h4>{{witness.titleName}}</h4>
                    <h5>{{witness.emailName}}</h5>
                    <div class="panel panel-danger">
                        <div class="panel-heading">Sightings</div>
                    </div>
                </div>
            </div>
        </div>
    {{/each}}
</div>